{
  "name": "gateway-service",
  "version": "1.0.0",
  "description": "This is a sample template for gateway-service - Below is a brief explanation of what we have generated for you:",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "symlink:linux": "mkdir -p .symlinks && ln -sFfT \"$(realpath ${PWD}/../graphql-service/apollo)\" .symlinks/graphql-service",
    "symlink:mac": "mkdir -p .symlinks && ln -sFh \"$(realpath ${PWD}/../graphql-service/apollo)\" .symlinks/graphql-service",
    "symlink": "(npm run symlink:mac) || (npm run symlink:linux)",
    "gen": "go run github.com/Khan/genqlient",
    "try-build-gateway": "cd gateway && go build && rm gateway",
    "try-build-cors": "cd cors && go build && rm cors",
    "try-build-all": "(npm run try-build-gateway) && (npm run try-build-cors)",
    "prebuild": "(npm run symlink) && (npm run gen) && (npm run try-build-all)",
    "build:x86": "(npm run prebuild) && (sam build --parameter-overrides Architecture=x86_64 --use-container --build-image=public.ecr.aws/sam/build-go1.x:latest-x86_64)",
    "build:arm64": "(npm run prebuild) && (sam build --parameter-overrides Architecture=arm64)",
    "build": "npm run build:arm64",
    "prestart": "npm run build && npm run genenvfile",
    "start": "sam local start-api -p 6000 --warm-containers eager --env-vars .samdevenv.json",
    "poststart": "delenvfile > .samdevenv.json",
    "watch": "nodemon -e go,graphql,yaml -i __generated__/ --signal SIGINT -x 'npm run start || exit 1'",
    "logs": "sam logs --tail",
    "logs:live": "sam logs --config-env live --tail",
    "deploy": "npm run build && sam deploy",
    "deploy:live": "npm run build && sam deploy --config-env live",
    "typecheck": "(npm run gen) && (npm run try-build-all)",
    "test": "jest",
    "genenvfile": "npx ts-node-esm printEnvFile.ts > .samdevenv.json",
    "delenvfile": "rm .samdevenv.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1343.0",
    "chalk": "^5.2.0",
    "jest": "^29.4.3",
    "node-fetch": "^2.6.9",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "@types/tmp": "^0.2.3",
    "tmp": "^0.2.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {}
}
