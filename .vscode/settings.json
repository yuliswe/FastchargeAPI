{
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": [
        "-l"
      ],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": [
        "-l"
      ],
      "env": {
        "ZDOTDIR": "${workspaceFolder}"
      }
    },
    "fish": {
      "path": "fish",
      "args": [
        "-l"
      ]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "files.exclude": {
    ".nodevenv": true,
    ".venv": true,
    ".zcompdump": true,
    ".zsh_history": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.symlinks/*": true,
    "**/CVS": true,
    "**/node_modules": true,
    "**/src/build/": true,
    "**/Thumbs.db": true
  },
  "python.analysis.importFormat": "relative",
  "explorerExclude.backup": {},
  "python.formatting.provider": "black"
}
