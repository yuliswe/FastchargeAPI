{
  "name": "fastchargeapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "eject": "react-scripts eject",
    "gen": "graphql-codegen --config codegen.yml && prettier -w ./src/__generated__/",
    "build": "npm run gen && react-scripts build",
    "test": "npm run gen && react-scripts test",
    "start": "npm run gen && PORT=8000 react-scripts start",
    "watch": "nodemon -e ts,tsx,yml,yaml -i build/ -i src/__generated__ --exec 'npm run gen' & PORT=8000 react-scripts start",
    "new-page": "node ../react-appevent-redux-tools/dist/index.js",
    "fix": "eslint --fix ./"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/client-sqs": "^3.279.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@mui/x-date-pickers": "^5.0.20",
    "chart.js": "^4.2.1",
    "commander": "^10.0.0",
    "date-fns": "^2.29.3",
    "firebase": "^9.16.0",
    "firebaseui": "^6.0.2",
    "jose": "^4.12.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-appevent-redux": "github:ylilarry/react-appevent-redux#dist",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.8.1",
    "react-router-hash-link": "^2.4.3",
    "react-terminal-ui": "github:ylilarry/react-terminal-ui",
    "sort-by": "^1.2.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/uuid": "^9.0.1",
    "react-scripts": "^5.0.1",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typescript-operations": "^3.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
