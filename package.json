    "jest-extended": "^4.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-transformer-svg": "^2.0.2",
    "json5": "^2.2.3",
    "node-fetch": "^2.7.0",
    "nodeman": "^1.1.2",
    "object-hash": "^3.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "ts-interface-builder": "^0.3.3",
    "ts-interface-checker": "^1.0.2",
    "ts-jest": "^29.1.2",
