{
  "name": "fastchargeapi-workspace",
  "version": "1.0.0",
  "description": "Workspace for FastchargeAPI source code.",
  "scripts": {
    "run-ts": "node -r esbuild-register --loader esbuild-register/loader",
    "lint-configs": "npm run run-ts ./project-linter.ts",
    "echo-build-order": "echo auth search graphql payment gateway echo",
    "test": "for s in $(npm run --silent echo-build-order | sed 's/ /\\n/g'); do (cd ${s}-service && npm run test); done",
    "deploy": "for s in $(npm run --silent echo-build-order | sed 's/ /\\n/g'); do (cd ${s}-service && npm run deploy); done",
    "deploy:live": "for s in $(npm run --silent echo-build-order | sed 's/ /\\n/g'); do (cd ${s}-service && npm run deploy:live); done"
  },
  "repository": {},
  "keywords": [],
  "author": "Yu Li",
  "license": "MIT",
  "bugs": {},
  "homepage": "",
  "devDependencies": {
    "@glen/jest-raw-loader": "^2.0.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@inrupt/jest-jsdom-polyfills": "^3.2.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node-fetch": "^2.6.11",
    "@types/object-hash": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.4.4",
    "esbuild": "^0.19.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.47.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^32.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-rewrite-imports": "^0.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-detect-case-change": "^1.0.0",
    "glob": "^10.3.10",
    "graphql": "^16.7.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
