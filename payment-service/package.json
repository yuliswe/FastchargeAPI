{
  "name": "payment-service",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "sam build",
    "deploy": "npm run build && sam deploy",
    "stripe": "stripe listen --forward-to https://api.payment.fastchargeapi.com/accept -e 'checkout.session.completed,checkout.session.expired,checkout.session.async_payment_succeeded,checkout.session.async_payment_failed' & stripe listen --forward-to https://api.payment.fastchargeapi.com/onboard/accept -e 'account.updated'",
    "logs": "sam logs --stack-name payment-service --tail"
  },
  "author": "",
  "license": "ISC"
}
