{
  "name": "fastchargeapi-cli-v2",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "type": "module",
  "scripts": {
    "test": "npx jest",
    "gen:ti": "ts-interface-builder --outDir=src/__generated__ ./src/types/*",
    "gen:gql": "npx graphql-codegen -c ./codegen.yml",
    "gen": "npm run gen:ti && npm run gen:gql",
    "prebuild": "npm run gen",
    "build": "node ./build.js",
    "watch": "nodemon --config ./nodemon.json",
    "fastcharge": "node --enable-source-maps ./dist/fastcharge.cjs",
    "fastapi": "node --enable-source-maps ./dist/fastapi.cjs"
  },
  "author": "",
  "license": "ISC",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@apollo/client": "^3.9.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-fetch": "^2.6.11",
    "@types/node-jose": "^1.1.13",
    "@types/opener": "^1.4.3",
    "@types/pem": "^1.14.4",
    "@types/uuid": "^9.0.8",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "decimal.js-light": "^2.5.1",
    "dotenv": "^16.4.1",
    "glob": "^10.3.10",
    "graphql-service-apollo": "file:../graphql-service/apollo",
    "jose": "^5.2.1",
    "opener": "^1.5.2",
    "pem": "^1.14.8",
    "react": "^18.2.0",
    "ts-interface-builder": "^0.3.3",
    "ts-interface-checker": "^1.0.2",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "typescript": "^4.9.5"
  }
}
